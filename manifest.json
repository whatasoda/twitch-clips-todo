{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "key": "",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "alarms"
  ],
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*",
        "https://twitch.tv/*"
      ],
      "js": [
        "src/content/index.ts"
      ]
    }
  ],
  "commands": {
    "record-moment": {
      "suggested_key": {
        "default": "Alt+Shift+C"
      },
      "description": "__MSG_manifest_commandRecordMoment__"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_title": "Twitch Clip Todo",
    "default_popup": "src/popup/index.html",
    "default_icon": {
      "16": "icons/icon-light-16.png",
      "32": "icons/icon-light-32.png",
      "48": "icons/icon-light-48.png"
    }
  }
}
